{
   "manifest_version": 3,
   "name": "Same Tab",
   "permissions": [ 
      "tabs", 
      "declarativeNetRequest"
   ],
   "version": "3.2.1",
   "background": {
      "service_worker": "background.js"
   },
   "declarative_net_request" : {
      "rule_resources" : [{
        "id": "rules",
        "enabled": true,
        "path": "rules.json"
      }]
    },  
   "description": "Forces link clicks from pinned tabs to always open in new tab"
}

